{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "types": [
      "@cloudflare/workers-types",
      "vite/client"
    ],
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@tg/*": [
        "src/party/src/*"
      ],
      "@app/*": [
        "src/*"
      ],
      "@static/*": [
        "static/*"
      ],
    }
  }
}